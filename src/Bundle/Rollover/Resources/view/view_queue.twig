{% extends '_base/_page-nav.twig' %}


{% block page_nav title %}

{% block page_title __(title) %}


{% block page_main %}



{% form_theme oForm 'form_bolt_layout.twig' %}
<div class="bm-search">
    <span class="bm-search_heading">Find Schedules</span>


    <form id="queueSearchForm" method="get" action="{{ path('bookme-queue-page')  }}"  class="bm-search_form form-inline">
        {{ form_widget(oForm) }}
       
       
        <input type="submit" value="Find Schedules" name="submit" class="btn btn-primary bm-search_submit">
    </form>

</div>



<div class="bm-table">

    <table id="QueueActivityTable" class="table table-striped table-bordered table-hover">
        
       
    </table>

</div>

{% endblock page_main %}

{% block page_script %}

    {% include '@BookMe/_base/_datatable.twig' with {'sConfigString' : sConfigString , 'sElement' : '#QueueActivityTable' , 'aEvents' : aEvents } %}

{% endblock page_script %}

